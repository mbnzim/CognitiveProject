
@{
    ViewBag.Title = "getPropeties";
}
<style>
    table, th, td {
        border: 1px solid black;
        border-collapse: collapse;
    }

    th, td {
        padding: 15px;
        text-align: left;
    }

    /*table#t01 {
        width: 100%;
        background-color: #f1f1c1;
    }*/
</style>

<h2>Image Propeties</h2>
<table style="width:50%">
    <tr>
        <td><b>URL</b></td>
        <td>@ViewBag.URL</td>
    </tr>
    <tr>
        <td><b>Image Name</b></td>
        <td>@ViewBag.blobname</td>
    </tr>
    <tr>
        <td><b>Size</b></td>
        <td>@ViewBag.Legnth</td>
    </tr>
    <tr>
        <td><b>Blob Type</b></td>
        <td>@ViewBag.BlobType</td>
    </tr>
    <tr>
        <td><b>ETag</b></td>
        <td>@ViewBag.ETag</td>
    </tr>
    <tr>
        <td><b>Created</b></td>
        <td>@ViewBag.Created</td>
    </tr>
    <tr>
        <td><b>LastModified</b></td>
        <td>@ViewBag.LastModified</td>
    </tr>
</table>


